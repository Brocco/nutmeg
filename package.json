{
  "name": "@nutmeg/cli",
  "version": "0.2.4",
  "description": "Build, test, and publish vanilla Web Components with a little spice",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "nutmeg": "bin/nutmeg",
    "nutmeg-new": "bin/nutmeg-new"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "element-template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/nutmeg-cli.git"
  },
  "keywords": [
    "web-components",
    "webcomponents",
    "shadow-dom",
    "shadowdom",
    "lit-html",
    "nutmeg",
    "typescript"
  ],
  "author": {
    "name": "Abraham Williams",
    "email": "abraham@abrah.am",
    "url": "https://abrah.am"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abraham/nutmeg-cli/issues"
  },
  "homepage": "https://github.com/abraham/nutmeg-cli",
  "scripts": {
    "build": "webpack",
    "prepare": "npm run build",
    "pretest": "mkdir tmp",
    "test": "cd tmp && ../bin/nutmeg new ci-test first:number second:string third:boolean && cd ci-test && npm run test",
    "watch": "webpack --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "webpack": "3.10.0",
    "ts-loader": "3.2.0",
    "typescript": "2.6.2",
    "@types/lodash.template": "4.4.3",
    "@types/node": "8.0.57",
    "@types/shelljs": "0.7.6",
    "@types/through2": "2.0.33",
    "commander": "2.12.2",
    "lodash.template": "4.4.0",
    "mocha": "4.0.1",
    "pascal-case": "2.0.1",
    "recursive-copy": "2.0.7",
    "shelljs": "0.7.8",
    "through2": "2.0.3"
  },
  "browser": {
    "fs": false,
    "child_process": false
  }
}
