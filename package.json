  },
  "devDependencies": {
    "@types/chai": "4.1.3",
    "@types/mocha": "5.2.0",
    "@webcomponents/webcomponentsjs": "1.2.0",
    "chai": "4.1.2",
    "karma": "2.0.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-sinon": "1.0.5",
    "karma-typescript": "3.0.12",
    "karma-typescript-es6-transform": "1.0.4",
    "mocha": "5.1.1",
    "sinon": "5.0.3",
    "typescript": "2.8.3",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
