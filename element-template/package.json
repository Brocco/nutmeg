{
  "name": "<%= tag %>",
  "version": "0.1.0",
  "description": "Generated Nutmeg Web Component",
  "main": "dist/<%= tag %>.js",
  "module": "dist/<%= tag %>.js",
  "unpkg": "dist/<%= tag %>.min.js",
  "types": "dist/<%= tag %>.d.ts",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "<%= tag %>",
    "nutmeg"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx nutmeg build .",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "serve": "npx nutmeg serve .",
    "test": "npx nutmeg test .",
    "watch": "npx nutmeg watch ."
  },
  "dependencies": {
    "@nutmeg/element": "0.3.0"
  },
  "devDependencies": {
    "@nutmeg/cli": "<%= cliSource %>",
    "@types/chai": "4.1.0",
    "@types/karma-fixture": "0.2.3",
    "@types/mocha": "2.2.46",
    "typescript": "2.6.2"
  }
}
